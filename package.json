{
  "name": "trophy-hunter",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "ts-node-dev --project tsconfig.server.json src/server.ts",
    "client:build": "vite build",
    "server:build": "tsc --project tsconfig.server.json",
    "build": "npm run server:build && npm run client:build",
    "prepare": "husky install",
    "test": "tsc && prettier --check . && eslint . && stylelint \"**/*.css\"",
    "start": "node dist/server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmachens/trophy-hunter.git"
  },
  "author": "Leon Machens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lmachens/trophy-hunter/issues"
  },
  "homepage": "https://github.com/lmachens/trophy-hunter#readme",
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "ajv": "^8.6.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "isomorphic-unfetch": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.0.0",
    "mongodb-cron": "^1.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lottie": "^1.2.3",
    "react-markdown": "^6.0.2",
    "react-query": "^3.19.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@overwolf/types": "git+https://github.com/lmachens/types.git#684d1bd6e756472aaac0e64046316b5bf85dd4cc",
    "@types/express": "^4.17.13",
    "@types/node": "^16.3.3",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vitejs/plugin-react-refresh": "^1.3.5",
    "concurrently": "^6.2.0",
    "eslint": "^7.31.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "patch-package": "^6.4.7",
    "prettier": "^2.3.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5",
    "vite": "^2.4.2"
  }
}
